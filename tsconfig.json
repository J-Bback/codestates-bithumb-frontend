{
  "compilerOptions": {
    "experimentalDecorators": true,
    "module": "esnext",
    "target": "esnext",
    "jsx": "preserve",
    "sourceMap": false,
    "moduleResolution": "node",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "typeRoots": [
      "node_modules/@types",
      "@types"
    ],
    "incremental": true
  },
  "exclude": [
    "node_modules",
    ".next"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts",
    ".eslintrc.js"
  ]
}
